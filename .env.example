# Botpress Configuration for Enhanced Chat Widget
# Copy this file to .env and fill in your actual Botpress credentials

# Your Botpress Bot ID
REACT_APP_BOTPRESS_BOT_ID=your-bot-id-here

# Your Botpress Host URL  
REACT_APP_BOTPRESS_HOST_URL=https://your-bot.botpress.cloud

# Your Botpress Messaging URL
REACT_APP_BOTPRESS_MESSAGING_URL=https://messaging.botpress.cloud

# Your Botpress Client ID
REACT_APP_BOTPRESS_CLIENT_ID=your-client-id-here
